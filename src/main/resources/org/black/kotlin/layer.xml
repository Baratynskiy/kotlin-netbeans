<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>

<folder name="Editors">
        <folder name="text">
            <folder name="x-kt">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.black.kotlin.Bundle"/>
                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="FontAndColors.xml" url="FontAndColors.xml">
                                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.black.kotlin.Bundle"/>
                            </file>
                        </folder>
                    </folder>
                </folder>
                
            </folder>
        </folder>
</folder>

    <folder name="OptionsDialog">
        <folder name="PreviewExamples">
            <folder name="text">
                <file name="x-kt" url="KtTemplate.kt"/>
            </folder>
        </folder>
    </folder>


    <folder name="Templates">
        <folder name="Kotlin">
            <file name="Kotlin.kt" url="Kotlin file">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.black.kotlin.Bundle"/>
                <attr name="template" boolvalue="true"/>
            </file>
        </folder>
    </folder>

    <folder name="Projects">
        <folder name="org-black-kotlin">
            <folder name="Customizer">
                <file name="Sources.instance">
                    <attr name="instanceCreate" methodvalue="org.black.kotlin.project.ui.customizer.KotlinCompositePanelProvider.createSources"/>
                    <attr name="position" intvalue="100"/>
                </file>
                <file name="Libraries.instance">
                    <attr name="instanceCreate" methodvalue="org.black.kotlin.project.ui.customizer.KotlinCompositePanelProvider.createLibraries"/>
                    <attr name="position" intvalue="200"/>
                </file>
<!--                <<folder name="BuildCategory">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.black.kotlin.project.ui.customizer.Bundle"/>
                    <attr name="position" intvalue="300"/>
                    <file name="Build.instance">
                        <attr name="instanceCreate" methodvalue="org.black.kotlin.project.ui.customizer.KotlinCompositePanelProvider.createBuild"/>
                        <attr name="position" intvalue="100"/>
                    </file>
                    <file name="Jar.instance">
                        <attr name="instanceCreate" methodvalue="org.black.kotlin.project.ui.customizer.KotlinCompositePanelProvider.createJar"/>
                        <attr name="position" intvalue="200"/>
                    </file>
                    <file name="Javadoc.instance">
                        <attr name="instanceCreate" methodvalue="org.black.kotlin.project.ui.customizer.KotlinCompositePanelProvider.createJavadoc"/>
                        <attr name="position" intvalue="300"/>
                    </file>
                </folder>-->
                <file name="Run.instance">
                    <attr name="instanceCreate" methodvalue="org.black.kotlin.project.ui.customizer.KotlinCompositePanelProvider.createRun"/>
                    <attr name="position" intvalue="400"/>
                </file>
                <folder name="Application">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.black.kotlin.project.ui.customizer.Bundle"/>
                    <attr name="position" intvalue="500"/>
                    <file name="Self.instance">
                        <attr name="instanceCreate" methodvalue="org.black.kotlin.project.ui.customizer.KotlinCompositePanelProvider.createApplication"/>
                        <attr name="position" intvalue="0"/>
                    </file>
                </folder>

            </folder>
        </folder>
    </folder>

</filesystem>
