            </folder>
        </folder>
    </folder>
    
    <folder name="Editors">
            <folder name="text">
                <folder name="x-kt">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.jetbrains.kotlin.Bundle"/>
