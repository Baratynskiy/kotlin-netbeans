        </folder>
        <folder name="org-netbeans-modules-maven">
            <folder name="Lookup">
                <file name="org.jetbrains.kotlin.projectsextensions.maven.lookup.MavenProjectLookupProviderExtension.instance"/>
            </folder>
        </folder>
    </folder>
    
    <folder name="Loaders">
        <folder name="text">
